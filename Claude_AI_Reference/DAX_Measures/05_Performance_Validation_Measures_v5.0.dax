Critical Data Alerts = 
// Critical data quality alerts summary
VAR RentRollAlert = 
    IF([Current Monthly Rent] < 1000000 || [Current Monthly Rent] > 10000000, "üî¥ RENT ROLL", "")
VAR LeasingAlert = 
    IF([Leasing Data Quality Score %] < 90, "üî¥ LEASING DATA", "")
VAR CreditAlert = 
    IF([Credit Score Coverage %] < 70, "üî¥ CREDIT DATA", "")
VAR AlertList = 
    CONCATENATE(
        CONCATENATE(RentRollAlert, IF(RentRollAlert <> "" && LeasingAlert <> "", " | ", "")),
        CONCATENATE(LeasingAlert, IF((RentRollAlert <> "" || LeasingAlert <> "") && CreditAlert <> "", " | ", ""))
    ) & CreditAlert
RETURN 
    IF(AlertList = "", "‚úÖ NO CRITICAL ALERTS", "‚ö†Ô∏è ALERTS: " & AlertList)

Performance Alerts = 
// Performance-related alerts
VAR SlowQueries = 
    IF([Total Leasing Deals] > 5000 && [Count Tenants with Credit Scores] > 1000, "üü° HIGH DATA VOLUME", "")
VAR ComplexityAlert = 
    IF([Current Leased SF] > 10000000 && [Active Leasing Pipeline Count] > 200, "üü° COMPLEX CALCULATIONS", "")
VAR AlertSummary = 
    CONCATENATE(SlowQueries, IF(SlowQueries <> "" && ComplexityAlert <> "", " | ", "")) & ComplexityAlert
RETURN
    IF(AlertSummary = "", "‚úÖ PERFORMANCE OPTIMAL", "‚ö†Ô∏è " & AlertSummary)

Business Rule Alerts = 
// Business rule validation alerts
VAR OccupancyAlert = 
    IF([Physical Occupancy %] > 100 || [Physical Occupancy %] < 0, "üî¥ OCCUPANCY OUT OF RANGE", "")
VAR RentPSFAlert = 
    IF([Current Rent Roll PSF] > 20 || [Current Rent Roll PSF] < 2, "üî¥ RENT PSF UNUSUAL", "")
VAR WaltAlert = 
    IF([WALT (Months)] > 200 || [WALT (Months)] < 0, "üî¥ WALT OUT OF RANGE", "")
VAR AlertList = 
    CONCATENATE(
        CONCATENATE(OccupancyAlert, IF(OccupancyAlert <> "" && RentPSFAlert <> "", " | ", "")),
        CONCATENATE(RentPSFAlert, IF((OccupancyAlert <> "" || RentPSFAlert <> "") && WaltAlert <> "", " | ", ""))
    ) & WaltAlert
RETURN
    IF(AlertList = "", "‚úÖ BUSINESS RULES OK", "‚ö†Ô∏è " & AlertList)

